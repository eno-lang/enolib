    :key: language
    :value: eno
"./spec/blackbox/blackbox.spec.rb[1:2:1]":
  :type: :document
  :elements:
  - :type: :field
    :key: Date
    :value: 1st of November in the year 2017
  - :type: :multiline_field_begin
    :key: Book Description
    :value: |-
      Lacks a certain ...
      something
  - :type: :list
    :key: Shopping List
    :items:
    - :type: :list_item
      :value: Apples
    - :type: :list_item
      :value: Oranges
  - :type: :fieldset
    :key: Telephone Numbers
    :entries:
    - :type: :fieldset_entry
      :key: Ben
      :value: "+49 1943 24724784"
    - :type: :fieldset_entry
      :key: Rachel
      :value: "+59 3459 35935593"
    - :type: :fieldset_entry
      :key: Dave
      :value: "+69 9025 93593531"
