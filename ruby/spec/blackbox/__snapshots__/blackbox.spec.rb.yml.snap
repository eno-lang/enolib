    :key: Body
    :elements:
    - :type: :section
      :key: Limbs
      :elements:
      - :type: :section
        :key: Left Arm
        :elements:
        - :type: :field
          :key: "--format pretty"
          :value: Pretty formatting option
        - :type: :list
          :key: "> Friends"
          :items:
          - :type: :list_item
            :value: Jack the Ripper
          - :type: :list_item
            :value: Frankenstein
        - :type: :fieldset
          :key: Use `${arg}` style options
          :entries:
          - :type: :fieldset_entry
            :key: "'--output dir'"
            :value: Specify a different output directory
          - :type: :fieldset_entry
            :key: "'--filters disabled'"
            :value: Enable/disable filters
        - :type: :field
          :key: I
          :value: append allthe things
