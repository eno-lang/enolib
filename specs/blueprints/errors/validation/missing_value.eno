# Querying a fieldset entry for a required but missing value

-- input
fieldset:
entry =
-- input

-- javascript
eno.parse(input).fieldset('fieldset').entry('entry').requiredStringValue();
-- javascript

-- php
Eno\Parser::parse($input)->fieldset('fieldset')->entry('entry')->requiredStringValue();
-- php

-- python
enolib.parse(input).fieldset('fieldset').entry('entry').required_string_value()
-- python

-- ruby
Eno.parse(input).fieldset('fieldset').entry('entry').required_string_value
-- ruby

