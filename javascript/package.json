{
  "author": "Simon Repp",
  "devDependencies": {
    "esdoc": "^1.1.0",
    "esdoc-node": "^1.0.5",
    "esdoc-standard-plugin": "^1.0.0",
    "jest": "^27.5.1",
    "typescript": "^4.6.3"
  },
  "exports": {
    "import": "./lib/esm/main.js",
    "require": "./lib/cjs/main.js"
  },
  "files": [
    "lib/"
  ],
  "name": "enolib",
  "description": "An eno parsing library",
  "license": "MIT",
  "main": "./lib/cjs/main.js",
  "keywords": [
    "eno",
    "parser",
    "validation"
  ],
  "scripts": {
    "build": "npm run build:cjs; npm run build:esm",
    "build:cjs": "tsc -p tsconfig_cjs.json",
    "build:esm": "tsc -p tsconfig_esm.json",
    "performance": "npm run build; node performance/benchmark.js",
    "test": "npm run build; jest --watch",
    "test-esm": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "type": "module",
  "version": "0.8.2"
}
